database(scaler) { nowhere() {
}
grecord(scaler,"scaler")
{
	field(SCAN,"I/O Intr")
	field(FLNK,"scaler_calc1.PROC PP MS")
#	field(FLNK,"scaler_wait.PROC PP MS")
	field(FREQ,"10000000")
	field(OUT,"#C0 S0 @")
	field(TP,"1")
	field(PREC,"3")
	field(G1,"1") 		# The gate 1 is a preset gate
	field(NM1,"Clock") 	# Channel 1 : clock
}

